{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "kurs-pm-admin-web"
    },
    "env": {
      "value": "prod"
    },
    "secretNames": {
      "value": [
        "KURS_PM_API_KEY",
        "SESSION_SECRET",
        "SESSION_KEY",
        "REDIS_URI",
        "APPLICATIONINSIGHTS_CONNECTION_STRING",
        "BLOB_SERVICE_SAS_URL",
        "OIDC_APPLICATION_ID",
        "OIDC_CLIENT_SECRET",
        "OIDC_TOKEN_SECRET",
        "OIDC_CONFIGURATION_URL",
        "OIDC_CALLBACK_URL",
        "OIDC_CALLBACK_SILENT_URL",
        "OIDC_CALLBACK_LOGOUT_URL"
      ]
    },
    "environmentVariables": {
      "value": {
        "LOGGING_USER": "kurs-pm-admin-web",
        "SERVICE_PUBLISH": "/kursinfoadmin/pm",
        "SERVER_HOST_URL": "https://app.kth.se",
        "SERVER_PORT": "3000",
        "CAS_SSO_URI": "https://login.kth.se",
        "CM_HOST_URL": "https://www.kth.se/cm/",
        "KOPPS_URI": "https://app.kth.se/api/kopps/v2/?defaultTimeout=60000",
        "KURS_PM_API_URI": "https://api.kth.se/api/kurs-pm",
        "STORAGE_CONTAINER_NAME": "memo-blob-container",
        "STORAGE_URL": "https://kursinfostorageprod.blob.core.windows.net/memo-blob-container/",
        "NODE_ENV": "production",
        "CONSOLE_ENABLED": "false",
        "STDOUT_ENABLED": "true",
        "LOGGING_LEVEL": "INFO",
        "LOGGING_ACCESS_LOG": "true",
        "TOOLBAR_URL": "https://www.kth.se/social/toolbar/widget.js"
      }
    },
    "monitorPath": {
      "value": "/kursinfoadmin/pm/_monitor"
    },
    "websitePort": {
      "value": "3000"
    }
  }
}
